<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>entidade.cliente.controle.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>entidade.cliente.controle.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarClienteServlet</servlet-name>
        <servlet-class>entidade.cliente.controle.CadastrarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerPerfilServlet</servlet-name>
        <servlet-class>entidade.cliente.controle.VerPerfilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdmServlet</servlet-name>
        <servlet-class>entidade.administrador.controle.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerPerfilAdmServlet</servlet-name>
        <servlet-class>entidade.administrador.controle.VerPerfilAdmServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginBothServlet</servlet-name>
        <servlet-class>paginas.login.controle.LoginBothServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarClienteServlet</servlet-name>
        <url-pattern>/CadastrarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerPerfilServlet</servlet-name>
        <url-pattern>/VerPerfilServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdmServlet</servlet-name>
        <url-pattern>/LoginAdmServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerPerfilAdmServlet</servlet-name>
        <url-pattern>/VerPerfilAdmServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginBothServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
